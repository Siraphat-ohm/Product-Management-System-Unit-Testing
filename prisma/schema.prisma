// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Products {
  id String @id @default(uuid())
  name String
  category String
  price Decimal @db.Decimal(20,2)
  stock Int
}